<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">


    <FrameLayout
        android:id="@+id/main_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/top_delivery"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@drawable/header"
            android:gravity="center"
            android:text="배송현황"
            android:textColor="@android:color/black"
            android:textSize="17dp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/ic_glass"
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_marginLeft="325dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/ic_glass" />

        <Button
            android:id="@+id/ic_scan"
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_marginLeft="355dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/ic_scan" />


        <TextView
            android:id="@+id/completionlist_name"
            android:layout_width="34dp"
            android:layout_height="12dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="110dp"
            android:gravity="center_horizontal|top"
            android:text="고객명"
            android:textAppearance="@style/배송완료_상세" />

        <TextView
            android:id="@+id/completionlist_address"
            android:layout_width="50dp"
            android:layout_height="12dp"
            android:layout_marginLeft="75dp"
            android:layout_marginTop="110dp"
            android:gravity="center_horizontal|top"
            android:text="상세주소"
            android:textAppearance="@style/배송완료_상세" />

        <TextView
            android:id="@+id/completionlist_delivery"
            android:layout_width="50dp"
            android:layout_height="12dp"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="110dp"
            android:gravity="center_horizontal|top"
            android:text="배송"
            android:textAppearance="@style/배송완료_상세" />

        <TextView
            android:id="@+id/completionlist_Progress"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="330dp"
            android:layout_marginTop="110dp"
            android:gravity="center_horizontal|top"
            android:text="진행현황"
            android:textAppearance="@style/배송완료_상세" />

        <TextView
            android:id="@+id/subname1"
            android:layout_width="54dp"
            android:layout_height="19dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="70dp"
            android:gravity="center_horizontal|top"
            android:text="배송완료"
            android:textAppearance="@style/수취방법"
            android:textStyle="bold" />


        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/subname2"
                android:layout_width="45dp"
                android:layout_height="19dp"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="390dp"
                android:gravity="center_horizontal|top"
                android:text="미완료"
                android:textAppearance="@style/수취방법"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/Incomplete_name"
                android:layout_width="34dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="420dp"
                android:gravity="center_horizontal|top"
                android:text="고객명"
                android:textAppearance="@style/배송완료_상세" />

            <TextView
                android:id="@+id/Incomplete_address"
                android:layout_width="50dp"
                android:layout_height="12dp"
                android:layout_marginLeft="75dp"
                android:layout_marginTop="420dp"
                android:gravity="center_horizontal|top"
                android:text="상세주소"
                android:textAppearance="@style/배송완료_상세" />

            <TextView
                android:id="@+id/Incomplete_dlivery"
                android:layout_width="50dp"
                android:layout_height="12dp"
                android:layout_marginLeft="280dp"
                android:layout_marginTop="420dp"
                android:gravity="center_horizontal|top"
                android:text="배송"
                android:textAppearance="@style/배송완료_상세" />

            <TextView
                android:id="@+id/Incomplete_progress"
                android:layout_width="50dp"
                android:layout_height="12dp"
                android:layout_marginLeft="330dp"
                android:layout_marginTop="420dp"
                android:gravity="center_horizontal|top"
                android:text="진행현황"
                android:textAppearance="@style/배송완료_상세" />


            <ListView
                android:id="@+id/dliveryStatus_scroll"
                android:layout_width="match_parent"
                android:layout_height="240dp"
                android:layout_marginTop="140dp">


            </ListView>

            <ListView
                android:id="@+id/dliveryStatus_scroll2"
                android:layout_width="match_parent"
                android:layout_height="240dp"
                android:layout_marginTop="440dp"></ListView>


        </FrameLayout>

        <LinearLayout
        android:id="@+id/linear_nav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="5dp"
        android:background="@drawable/border_linear4"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_to_map4"
            android:layout_width="82dp"
            android:layout_height="63dp"
            android:layout_alignParentBottom="true"
            android:background="#00ff0000"
            android:drawableTop="@drawable/img_location_gray"
            android:drawablePadding="-5dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:text="지도"
            android:textColor="#dfe2eb" />

        <Button
            android:id="@+id/btn_to_barcode4"
            android:layout_width="82dp"
            android:layout_height="63dp"
            android:layout_alignParentBottom="true"
            android:background="#00ff0000"
            android:drawableTop="@drawable/img_scan_gray"
            android:drawablePadding="-5dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:text="등록"
            android:textColor="#dfe2eb"/>

        <Button
            android:id="@+id/btn_to_request4"
            android:layout_width="82dp"
            android:layout_height="63dp"
            android:layout_alignParentBottom="true"
            android:background="#00ff0000"
            android:drawableTop="@drawable/img_addcircle_gray"
            android:drawablePadding="-5dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:text="업무 신청"
            android:textColor="#DFE2EB"/>

        <Button
            android:id="@+id/btn_to_delivery4"
            android:layout_width="82dp"
            android:layout_height="63dp"
            android:layout_alignParentBottom="true"
            android:background="#00ff0000"
            android:drawableTop="@drawable/img_car_black"
            android:drawablePadding="-5dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:text="배송현황"
            android:textColor="#000000"/>

        <Button
            android:id="@+id/btn_to_more4"
            android:layout_width="82dp"
            android:layout_height="63dp"
            android:layout_alignParentBottom="true"
            android:background="#00ff0000"
            android:drawableTop="@drawable/img_menu_gray"
            android:drawablePadding="-5dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:text="더보기"
            android:textColor="#dfe2eb" />

    </LinearLayout>
    </FrameLayout>
</FrameLayout>
